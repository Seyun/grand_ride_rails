<p id="notice"><%= notice %></p>

<p>
  <b>Passenger UUID:</b>
  <%= @ride.passenger_uuid %>
</p>


<img src="<%=raw @map_url %>" />
<br/>
<% @ride.tracks.each do |track| %>
    <p>
      <b>Time:</b>
      <%= track.created_at %>
      <br/>
      <b>Latitude:</b>
      <%= track.latitude %>
      <br/>
      <b>Longitude:</b>
      <%= track.longitude %>
      <br/>
    </p>
<%end%>
<%= link_to 'here', ride_url(@ride) %>
<%= link_to 'Edit', edit_ride_path(@ride) %> |
<%= link_to 'Back', rides_path %>
